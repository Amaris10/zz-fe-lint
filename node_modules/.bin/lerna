#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/amaris/Home/Code_me/FrontEndProject/zz-fe-lint/node_modules/.pnpm/registry.npmmirror.com+lerna@8.0.0/node_modules/lerna/dist/node_modules:/Users/amaris/Home/Code_me/FrontEndProject/zz-fe-lint/node_modules/.pnpm/registry.npmmirror.com+lerna@8.0.0/node_modules/lerna/node_modules:/Users/amaris/Home/Code_me/FrontEndProject/zz-fe-lint/node_modules/.pnpm/registry.npmmirror.com+lerna@8.0.0/node_modules:/Users/amaris/Home/Code_me/FrontEndProject/zz-fe-lint/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/amaris/Home/Code_me/FrontEndProject/zz-fe-lint/node_modules/.pnpm/registry.npmmirror.com+lerna@8.0.0/node_modules/lerna/dist/node_modules:/Users/amaris/Home/Code_me/FrontEndProject/zz-fe-lint/node_modules/.pnpm/registry.npmmirror.com+lerna@8.0.0/node_modules/lerna/node_modules:/Users/amaris/Home/Code_me/FrontEndProject/zz-fe-lint/node_modules/.pnpm/registry.npmmirror.com+lerna@8.0.0/node_modules:/Users/amaris/Home/Code_me/FrontEndProject/zz-fe-lint/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../lerna/dist/cli.js" "$@"
else
  exec node  "$basedir/../lerna/dist/cli.js" "$@"
fi
