#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/amaris/Home/Code_me/FrontEndProject/zz-fe-lint/node_modules/.pnpm/registry.npmmirror.com+import-local@2.0.0/node_modules/import-local/fixtures/node_modules:/Users/amaris/Home/Code_me/FrontEndProject/zz-fe-lint/node_modules/.pnpm/registry.npmmirror.com+import-local@2.0.0/node_modules/import-local/node_modules:/Users/amaris/Home/Code_me/FrontEndProject/zz-fe-lint/node_modules/.pnpm/registry.npmmirror.com+import-local@2.0.0/node_modules:/Users/amaris/Home/Code_me/FrontEndProject/zz-fe-lint/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/amaris/Home/Code_me/FrontEndProject/zz-fe-lint/node_modules/.pnpm/registry.npmmirror.com+import-local@2.0.0/node_modules/import-local/fixtures/node_modules:/Users/amaris/Home/Code_me/FrontEndProject/zz-fe-lint/node_modules/.pnpm/registry.npmmirror.com+import-local@2.0.0/node_modules/import-local/node_modules:/Users/amaris/Home/Code_me/FrontEndProject/zz-fe-lint/node_modules/.pnpm/registry.npmmirror.com+import-local@2.0.0/node_modules:/Users/amaris/Home/Code_me/FrontEndProject/zz-fe-lint/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmmirror.com+import-local@2.0.0/node_modules/import-local/fixtures/cli.js" "$@"
else
  exec node  "$basedir/../../../../../registry.npmmirror.com+import-local@2.0.0/node_modules/import-local/fixtures/cli.js" "$@"
fi
