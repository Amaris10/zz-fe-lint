#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmmirror.com+@nrwl+tao@17.1.3/node_modules/@nrwl/tao/index.js" "$@"
else
  exec node  "$basedir/../../../../../registry.npmmirror.com+@nrwl+tao@17.1.3/node_modules/@nrwl/tao/index.js" "$@"
fi
